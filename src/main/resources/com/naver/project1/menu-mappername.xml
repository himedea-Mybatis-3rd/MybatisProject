<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="EmployeeMapper">

    <resultMap id="employeeResultMap" type="com.naver.project1.model.dto.EmployeeDTOname">
        <id property="empId" column="EMP_ID"/>
        <result property="nationalCode" column="NATIONAL_CODE"/>
        <result property="nationalName" column="NATIONAL_NAME"/>
        <result property="jobCode" column="JOB_CODE"/>
        <result property="jobName" column="JOB_NAME"/>
        <result property="hireDate" column="HIRE_DATE"/>
        <result property="empName" column="EMP_NAME"/>
        <result property="deptId" column="DEPT_ID"/>
        <result property="deptTitle" column="DEPT_TITLE"/>

    </resultMap>
    <select id="selectAllMenu" resultMap="employeeResultMap">
        SELECT
            EMP_NAME
        FROM
            EMPLOYEE
        WHERE
            LEFT(HIRE_DATE,2) = 20;
    </select>

</mapper>